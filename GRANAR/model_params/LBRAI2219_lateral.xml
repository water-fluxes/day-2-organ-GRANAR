<?xml version="1.0" encoding="utf-8"?>

<granar>
	
	<!-- General parameters -->
	<planttype param="1" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="1" />
	
	<!-- Cell layers -->
	<stele cell_diameter="0.003" n_layers="1" layer_diameter = "0.06" order="1"/>
	<pericycle cell_diameter="0.003" n_layers="1"  order="2"/>
	<endodermis cell_diameter="0.006" n_layers="1"  order="3"/>
	<cortex_inner cell_diameter="0.010" n_layers="1"  order="4"/>
	<cortex cell_diameter="0.014" n_layers="2"  order="4"/>
	<exodermis cell_diameter="0.01" n_layers="1" order="5"/>	
	<epidermis cell_diameter="0.008" n_layers="1" order="6"/>


	<!-- Vessels-->
	<xylem max_size="0.01" n_files="1" order="1.5" ratio ="1.21" />
	<phloem max_size="0.01" n_files="2" />
	
	<!-- Other cell types -->
	<!--aerenchyma proportion="0" n_files="10"/-->
	<aerenchyma proportion="1.0" n_files="2"/>

</granar>
